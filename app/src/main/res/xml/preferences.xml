<?xml version="1.0" encoding="utf-8"?>
<PreferenceScreen xmlns:android="http://schemas.android.com/apk/res/android">

    <PreferenceCategory android:layout="@layout/custom_toolbar_layout"></PreferenceCategory>

    <SwitchPreferenceCompat
        android:defaultValue="true"
        android:key="@string/demo_data_key"
        android:title="@string/demo_data_title"
        android:summary="@string/demo_data_summary"
        android:enabled="false"/>
    <SwitchPreferenceCompat
        android:defaultValue="false"
        android:key="@string/orientation_key"
        android:title="@string/orientation_title"
        android:summary="@string/orientation_summary"
        android:enabled="false"/>
    <ListPreference
        android:defaultValue="1"
        android:entries="@array/array_updatePreferences"
        android:entryValues="@array/array_updateValues"
        android:key="@string/update_rate_key"
        android:title="@string/update_rate_title"
        android:summary="@string/update_rate_summary"/>
    <Preference
        android:title="@string/clear_app_cache_title"
        android:key="@string/clear_app_cache_key" />
    <Preference
        android:title="@string/reset_app__title"
        android:key="@string/reset_app__key"/>
    <Preference
        android:title="@string/server_address_title"
        android:summary="@string/server_address_summary"
        android:key="@string/server_address_key"/>
    <Preference
        android:title="@string/build_title"
        android:key="buildVersion"/>

</PreferenceScreen>