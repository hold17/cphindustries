<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="dk.blackdarkness.g17.cphindustries">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:logo="@drawable/ic_ci_logo_small_trans"
        android:roundIcon="@drawable/ic_ci_logo_small_trans"
        android:supportsRtl="true"
        android:theme="@style/CPHIndustriesTheme">
        <activity
            android:name=".activities.SceneViewActivity"
            android:label="@string/app_name"
            android:screenOrientation="portrait"
            android:theme="@style/CPHIndustriesTheme">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".activities.SettingsActivity"
            android:label="@string/title_activity_settings"></activity>
    </application>

</manifest>